{
  "name": "leantony/laravel-grid",
  "description": "A grid view for laravel, inspired by the yii2 grid widget",
  "license": "MIT",
  "type": "library",
  "keywords": [
    "leantony",
    "bootstrap",
    "laravel-grid",
    "laravel",
    "pjax",
    "grid",
    "laravel-5",
    "laravel5"
  ],
  "homepage": "https://github.com/leantony/laravel-grid",
  "authors": [
    {
      "name": "Antony [leantony] Chacha",
      "email": "chachaantony@gmail.com",
      "role": "Developer"
    }
  ],
  "require": {
    "illuminate/view": "~5.5",
    "illuminate/support": "~5.5",
    "illuminate/console": "~5.5",
    "illuminate/contracts": "~5.5",
    "illuminate/http": "~5.5",
    "illuminate/database": "~5.5",
    "illuminate/pagination": "~5.5"
  },
  "suggest": {
    "maatwebsite/excel": "For exporting grid data as excel (~3.0)",
    "spatie/laravel-pjax": "For pjax support on the server side"
  },
  "require-dev": {
    "phpunit/phpunit": "~7.0",
    "orchestra/testbench": "~3.0",
    "orchestra/testbench-dusk": "^3.5"
  },
  "autoload": {
    "psr-4": {
      "Leantony\\Grid\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "Leantony\\Grid\\Providers\\GridServiceProvider"
      ],
      "aliases": {
        "Modal": "Leantony\\Grid\\Facades\\Modal"
      }
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
